people_between = 0  # Счётчик людей между Александрой и Левоном
found_alexandra = False  # Флаг нашли ли Александру

# Цикл для чтения имён из очереди пока не найдем Левона
while True:
    name = input("Введите имя человека в очереди: ")

    # Проверяем нашли ли Левона
    if name == "Левон":
        break  # Если нашли прекращаем подсчёт

    # Проверяем нашли ли Александру
    if name == "Александра":
        found_alexandra = True
        continue  # Александру не считаем переходим к след.

    # Если Александра уже нашли то считаем людей после неё
    if found_alexandra:
        people_between += 1

print(people_between)
